{
  "builds": [
    {
      "src": "kuyofrontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "kuyofrontend/dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/kuyofrontend/dist/$1"
    }
  ]
}
